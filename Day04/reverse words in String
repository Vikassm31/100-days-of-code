public class Main{
    public static void main(String[] args){
        String s="palle tech pvt ltd";
        String[] c=s.split(" ");
        String fin="";
        for(int i=0;i<c.length;i++){
            String str=c[i];
            String st="";
            for(int j=str.length()-1;j>=0;j--){
                st=st+str.charAt(j);
            }
            fin=fin+" "+st;
        }
        System.out.println(fin);
    }
}
