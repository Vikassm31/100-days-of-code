public class SecondBig {

	public static void main(String[] args) {
		int[] a= {44,21,15,14,28,32,14,17,6,21};
		int big=Integer.MIN_VALUE;int second=Integer.MIN_VALUE;
		for(int i=0;i<a.length;i++) {
			if(a[i]>big) {
				second=big;
				big=a[i];
			}
			else if(a[i]>second && a[i]<big) {
				second=a[i];
			}
		}
		System.out.println("biggest "+big);
		System.out.println("second biggest "+second);
	}

}
